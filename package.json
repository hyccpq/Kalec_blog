  "scripts": {
    "dev:index": "NODE_ENV=development:index nodemon index.js",
    "dev:manage": "NODE_ENV=development:manage nodemon index.js",
    "node": "nodemon index.js",
    "build": "rm -rf public/dist && rm -rf .cache && yarn build:client && yarn build:server && yarn build:manage && yarn build-routes",
    "build:client": "cross-env NODE_ENV=production webpack --config server/build/webpack.config.client.cjs --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config server/build/webpack.config.server.cjs --progress --hide-modules",
    "build:manage": "cross-env NODE_ENV=production webpack --config server/build/webpack.config.manage.cjs --progress --hide-modules",
    "start": "NODE_ENV=production node --es-module-specifier-resolution=node server/index.js",
    "production:test": "NODE_ENV=production:test node server/index.js",
    "test": "NODE_ENV=test node server/index.js",
    "format-server": "prettier -l --write 'server/**/*.{js,vue,stylus,css}'",
    "build-routes": "BUILD_ENV=server babel -d ./server/routes-build ./server/routes -s"
  },
